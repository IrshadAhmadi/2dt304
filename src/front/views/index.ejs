<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Snippet Manager</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <h1>Snippet Manager</h1>
    <% if (snippets.length > 0) { %>
        <% snippets.forEach(function(snippet) { %>
            <div>
                <h2><%= snippet.title %></h2>
                <p><%= snippet.content %></p>
            </div>
        <% }) %>
    <% } else { %>
        <p>No snippets found</p>
    <% } %>
</body>
</html>